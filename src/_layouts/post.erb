---
layout: default
---

<%= render "mobile_table_of_contents" %>

<main id="main" class="documentation-content">
  <% title_id = resource.data.title.downcase.split(" ").join("-").gsub(/[\?!]/, "") %>
  <h1 id="page-header__<%= title_id %>">
    <a href="<%= "##{title_id}" %>">
      <%= resource.data.title %>
    </a>
  </h1>

  <%= yield %>
</main>

<%= render "table_of_contents" %>
<%= render "pagination" %>

